@{
    ViewBag.Title = "登录";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


@section css{
    <link href="~/Content/page/loging.css" rel="stylesheet" />
    <link href="~/Content/page/logreset.css" rel="stylesheet" />

}

<!-- /top -->
<div id="g-doc">
    <!-- logo start-->
    <div class="g-hd clearfix">
        <div class="m-logo">
            <h1 class="logo">
                <a href="#"><img src="../../Content/images/logo.png" /></a>
            </h1>
            <a href><img src="../../Content/images/logo-1.png" /></a>
        </div>
        <p class="m-tt">欢迎登录</p>
    </div>
    <!-- /logo end -->
    <!-- body start -->
    <div class="g-bd clearfix">
        <div class="m-ad">
            <img src="../../Content/images/lodingBG.png" />
        </div>
        <!-- login start -->
        <div class="m-log">
            <form>
                <p class="m-tt">帐号登录</p>

                <p class="z-err">
                    <label>
                        <input class="u-err" type="text" value="帐户名与密码不匹配，请重新输入" />
                    </label>
                    <em class="err"></em>
                </p>

                <p class="m-ipt">
                    <input class="u-ipt" type="text" name="useName" placeholder="请输入用户名" />
                    <em class="abslu-1"></em>
                </p>

                <p class="m-ipt">
                    <input class="u-ipt" type="password" name="password" placeholder="请输入密码" />
                    <em class="abslu-2"></em>
                </p>

                <p class="m-ipt-wrap">
                    <input class="m-verify-box" type="text" placeholder="请输入验证码" />
                    <img class="m-verify" src="../../Content/images/identifying.png" />
                </p>

                <p class="m-tips">
                    <label>
                        <input class="check" type="checkbox" name="" />
                    </label>
                    <span class="check-2">下次自动登录</span>
                    <a href="#" class="forget">忘记密码？</a>
                </p>
                <button class="u-btn" onclick="scrop_login.onLogin()">登&nbsp&nbsp录</button>

                <p class="f-tac">还没有帐号？马上<a href="#">免费注册</a></p>
            </form>
        </div>
        <!-- /login end -->
    </div>
    <!-- /body end -->
</div>
<!-- 尾部开始 -->
@section script{

    <script src="~/Scripts/require.js" data-main="./Scripts/main.js" data-js="page/login"></script>

}
